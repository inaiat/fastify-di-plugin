{
  "name": "@inaiat/fastify-di-plugin",
  "version": "1.0.1",
  "description": "Simple, unobtrusive depency injection for fastify.",
  "type": "module",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/mjs/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf ./dist && mkdir dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && bash ./fixup && git rev-parse HEAD > BUILD_SHA",
    "lint": "xo --fix",
    "prepublishOnly": "npm run build",
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inaiat/fastify-di-plugin.git"
  },
  "keywords": [
    "fastify",
    "awilix",
    "dependency-injection",
    "dependency",
    "plugin",
    "di",
    "injection",
    "inversion",
    "control"
  ],
  "contributors": [
    "Inaiat <inaiat@gmail.com>"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/inaiat/fastify-di-plugin/issues"
  },
  "homepage": "https://github.com/inaiat/fastify-di-plugin",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "fastify-plugin": "^4.2.0"
  },
  "peerDependencies": {
    "awilix": "^7",
    "fastify": "^4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.57",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^18.16.0",
    "@types/semver": "^7.5.0",
    "ava": "^5.2.0",
    "awilix": "^8.0.1",
    "c8": "^7.13.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fastify": "^4.17.0",
    "mongodb-memory-server": "^8.12.2",
    "rimraf": "^5.0.0",
    "semver": "^7.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "xo": "^0.54.2"
  },
  "directories": {
    "test": "test"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "ignores": [
      "init-tracer.cjs"
    ],
    "plugins": [
      "prettier",
      "unused-imports"
    ],
    "rules": {
      "capitalized-comments": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-method-this-argument": "off",
      "new-cap": "off",
      "import/no-extraneous-dependencies": "off",
      "@typescript-eslint/member-delimiter-style": "off",
      "@typescript-eslint/prefer-readonly-parameter-types": "off",
      "@typescript-eslint/naming-convention": "off",
      "@typescript-eslint/consistent-type-imports": "off",
      "@typescript-eslint/consistent-type-definitions": "off",
      "@typescript-eslint/no-unsafe-assignment": "warn",
      "@typescript-eslint/no-empty-function": "warn",
      "@typescript-eslint/no-unsafe-call": "off",
      "@typescript-eslint/object-curly-spacing": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error"
    },
    "overrides": [
      {
        "files": "tests/**/*.ts",
        "rules": {
          "functional/no-method-signature": "off",
          "@typescript-eslint/prefer-readonly-parameter-types": "off",
          "@typescript-eslint/no-unsafe-assignment": "off",
          "functional/prefer-readonly-type": "off",
          "ava/use-test": "off",
          "unicorn/no-await-expression-member": "off",
          "prefer-promise-reject-errors": "off"
        }
      }
    ]
  },
  "author": "inaiat@gmail.com"
}